{
    "name": "angular-firebase-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "e2e": "testcafe chrome e2e/fixtures --disable-multiple-windows",
        "e2e:slow": "testcafe chrome e2e/fixtures --disable-multiple-windows --speed 0.7",
        "format": "prettier --write '**/*.{ts,js,scss,html,css,md,json}'",
        "start": "ng serve & npm run e2e",
        "serve": "./stop.sh && ng serve",
        "start:slow": "ng serve & npm run e2e:slow",
        "build": "ng build",
        "stop": "./stop.sh",
        "test": "ng test",
        "lint": "ng lint"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.12",
        "@angular/common": "~8.2.12",
        "@angular/compiler": "~8.2.12",
        "@angular/core": "~8.2.12",
        "@angular/fire": "^5.2.1",
        "@angular/forms": "~8.2.12",
        "@angular/platform-browser": "~8.2.12",
        "@angular/platform-browser-dynamic": "~8.2.12",
        "@angular/router": "~8.2.12",
        "firebase": "^7.2.2",
        "firebase-tools": "^7.6.2",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.15",
        "@angular/cli": "~8.3.15",
